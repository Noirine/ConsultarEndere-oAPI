@model Endereco
    <!--Ariane Noirine Pimentel Silva Assis Neves, Matricula 1621586-->

    <!DOCTYPE html>

    <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Inserir Complemento</title>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
    </head>
    <body class="container" style="padding-top:30px">

        <form asp-action="Alterar" asp-controller="Endereco" class="col-6 ">
            <fieldset>
                <legend>Inserir Complemento</legend>

                <div class="form-group">
                    <input type="hidden" asp-for="enderecoId" />
                </div>

                <div class="form-group">
                    <input type="hidden" asp-for="cep" class="form-control" required="required" />
                </div>

                <div class="form-group">

                    <input type="hidden" asp-for="logradouro" class="form-control" required="required" />
                </div>

                <div class="form-group">
                    <label asp-for="complemento">Complemento:</label>
                    <input type="text" asp-for="complemento" class="form-control" />
                </div>

                <div class="form-group">

                    <input type="hidden" asp-for="bairro" class="form-control" required="required" />
                </div>

                <div class="form-group">

                    <input type="hidden" asp-for="localidade" class="form-control" required="required" />
                </div>

                <div class="form-group">

                    <input type="hidden" asp-for="uf" class="form-control" required="required" />
                </div>


                <input type="submit" value="Alterar" class="btn btn-success" asp-for="enderecoId" />

            </fieldset>
        </form>

    </body>
</html>
